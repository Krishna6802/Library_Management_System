<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.booktrack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BookIssueAdapter"></activity>
        <activity android:name=".UserIssueBookActivity" />
        <activity android:name=".UserViewBookActivity" />
        <activity android:name=".UserViewCatActivity" />
        <activity android:name=".AdminEditBookActivity" />
        <activity android:name=".AdminEditCatActivity" />
        <activity android:name=".UserProfileActivity" />
        <activity android:name=".AdminManageUsers" />
        <activity android:name=".AdminManageBookReturnActivity" />
        <activity android:name=".AdminManageReqActivity" />
        <activity android:name=".AdminManageBookActivity" />
        <activity android:name=".AdminManageCatActivity" />
        <activity android:name=".AdminAddBookActivity" />
        <activity android:name=".AdminAddCatActivity" />
        <activity android:name=".AdminDashboardActivity" />
        <activity android:name=".AdminLoginActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".UserActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>